  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.646.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.10",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "rd": "^2.0.1",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2s --ignore '.scss' --ignore '.static",
    "dev:assets": "cd src && webpack --mode development -w",
    "build:assets": "cd src && webpack --mode production",
    "build:server": "babel src --out-dir build",
    "build": "npm run build:server && npm run build:assets && xcopy .\\src\\static .\\build\\static\\ /s /y && xcopy .\\src\\views .\\build\\views\\ /s /y",
    "prebuild": "rd /s /q build",
    "tunnel": "ngrok http 4000",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
