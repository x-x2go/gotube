extends layouts/main
include mixins/videoBlock

block content
    .search__header
        |Searching by  
        h3 #{searchingBy}
    .search__videos
        if videos.length ===0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                title: item.title,
                thumbnail: item.thumbnail,
                views: item.views,
                videoFile: item.videoFile,
                id: item.id
            })